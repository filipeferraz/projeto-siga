function funcHora(){
	var hr = document.getElementById('formAgendamento.hora_ag').value;
	hr = hr.substr(0,2)+hr.substr(3,2);
	alert(hr);
	document.getElementById('formAgendamento.hora_ag').value = hr;
	alert(document.getElementById('formAgendamento.hora_ag').value);
}
=================================================================================================
function foi(response){
    alert(“Foi” + response);
}

function naoFoi(response){
    alert(“Não foi”);
}

$.ajax({
       type: "POST",
       url: "@{Application.salvar()}",
       data: $(“#formulario”).serialize(),
       success: foi,
       error: naoFoi
});
